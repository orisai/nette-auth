{
	"name": "orisai/nette-auth",
	"description": "Orisai Auth integration for Nette",
	"keywords": [
		"access-control",
		"abac",
		"acl",
		"auth",
		"authentication",
		"authorization",
		"login",
		"nette",
		"orisai",
		"password",
		"passwords",
		"permissions",
		"security",
		"policy",
		"rbac",
		"role",
		"security",
		"users"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/nette-auth",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.3.0",
		"nette/http": "^3.1.5",
		"orisai/auth": "^1.0.0|^2.0.0",
		"orisai/exceptions": "^1.0.0",
		"orisai/nette-di": "^1.0.7"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.26.0",
		"orisai/coding-standard": "^3.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-nette": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0",
		"tracy/tracy": "^2.8.0"
	},
	"autoload": {
		"psr-4": {
			"OriNette\\Auth\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\OriNette\\Auth\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
